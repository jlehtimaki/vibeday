    "langchain",
    "ai-agent",
    "itinerary",
    "planning"
  ],
  "author": "Warden",
  "license": "UNLICENSED",
  "dependencies": {
    "@langchain/core": "^0.3.57",
    "@langchain/langgraph": "^0.3.0",
    "@langchain/openai": "^0.3.0",
    "typescript": "^5.6.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tsx": "^4.19.0",
    "vitest": "^2.1.0"
  },
  "engines": {
    "node": "22"
  }
}
