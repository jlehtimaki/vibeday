{
  "name": "vibeday",
  "version": "0.1.0",
  "description": "AI agent that generates date/micro-adventure itineraries",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "postinstall": "npm run build",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
